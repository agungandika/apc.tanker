{
    "name": "agungandika/sisteminventorybbm",
    "description": "Sistem Inventori BBM - Built with YII2 Advanced Template",
    "keywords": ["yii2", "framework", "inventory", "thesis"],
    "homepage": "http://www.agungandika.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/agungsijawir/sii/issues?state=open",
        "forum": "http://www.agungandika.com/demos/sii/",
        "source": "https://github.com/agungsijawir/sii"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
